import java.io.*;
public class Testing {
  public static void main (String [] args)throws IOException {
    PrintWriter outFile = new PrintWriter ("out.txt");
    int x = 4, y = 7, z, i, j = 3;
    String blah = "hello there people";
    outFile.println(blah);
    outFile.close();
    z = cube(x,y,j);
    i = (7 * 3 - x) / cube(y,x,z);
    System.out.println("z is "+z+" and i is "+i);
    printer();
    System.out.println("This program is now completed.");
  }
  public static int cube (int i, int j, int k){
    int t = i * j * k;
    return t;
  }
  public static void printer (){
    System.out.println("This is the printer method printing");
    return;
  }
}
    